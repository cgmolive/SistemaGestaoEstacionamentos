@model IEnumerable<SistemaDeEstacionamentos.Model.TabelaDePreco>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ConsultarTabelaDePreco</title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <th>
                        Id Estacionamento
                    </th>
                    <th>
                        Valor de 4 horas
                    </th>
                    <th>
                        Valor da hora adicional
                    </th>
                    <th>
                        Diária
                    </th>
                    <th>
                        Carencia
                    </th>
                    <th>

                    </th>
                </tr>
            </thead>

            <td>@ViewBag.Tabelas.EstacionamentoId</td>
            <td>@ViewBag.Tabelas.AteQuatroHoras</td>
            <td>@ViewBag.Tabelas.AdicionalPorHora</td>
            <td>@ViewBag.Tabelas.ValorDiaria</td>
            <td>@ViewBag.Tabelas.Carencia</td>
            <td><button id=" =Editar" name=" Editar" class=" btn btn-primary" onclick="window.location.href = '/Estacionamentos/EditarTabela?Handle=@ViewBag.Tabelas.Handle';">Editar tabela</button></td>
        </table>
    </div>
    <td><button id=" =Editar" name=" Voltar" class=" btn btn-primary" onclick="window.location.href = '/Estacionamentos/Index';">Voltar</button></td>
</body>
</html>
