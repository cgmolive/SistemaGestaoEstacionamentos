@model IEnumerable<SistemaDeEstacionamentos.Usuarios>

@{
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>

</head>
<body>
    <table class="table">

        <thead>
            <tr>
                <th>Nome</th>
                <th>CEP</th>
                <th>CPF</th>
                <th>Usuario</th>
            </tr>
        </thead>

        <tbody>
            @foreach(var usuarios in ViewBag.usuarios)
            {
            <tr>
                <td>@usuarios.Nome</td>
                <td>@usuarios.Cpf</td>
                <td>@usuarios.cep</td>
                <td>@usuarios.nomeDeUsuario</td>


            </tr>
            }
        </tbody>

    </table>
</body>
</html>
